{
  "name": "focal-todomvc",
  "private": true,
  "version": "0.0.0",
  "main": "webpack/dev_server.js",
  "engines": {
    "node": ">=4.0.0",
    "npm": ">=3.0.0"
  },
  "sideEffects": false,
  "scripts": {
    "build:webpack": "NODE_ENV=production webpack --config webpack/webpack.prod.js",
    "build": "npm run clean && npm run build:static && npm run build:webpack",
    "build:static": "cp -r ./static ./build",
    "clean": "rm -rf ./build",
    "dev": "npm start",
    "start": "node ./webpack/dev_server.js",
    "test": "yarn build",
    "lint": "eslint './src/**/*.ts*' && tsc --noemit"
  },
  "author": "Grammarly, Inc.",
  "license": "Apache-2.0",
  "devDependencies": {
    "@grammarly/focal": "0.11.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.0",
    "eslint": "^7.18.0",
    "eslint-webpack-plugin": "4.0.1",
    "express": "^4.14.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.0",
    "rxjs": "^7.8.1",
    "todomvc-app-css": "^2.0.6",
    "ts-loader": "8.2.0",
    "typescript": "^4.7.4",
    "webpack": "5.88.1 ",
    "webpack-bundle-analyzer": "4.9.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-middleware": "6.1.1",
    "webpack-hot-middleware": "2.25.4"
  }
}
